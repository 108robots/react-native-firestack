{
  "name": "Firestack",
  "version": "0.0.5",
  "keywords": [
    "react-native",
    "react",
    "native",
    "firebase"
  ],
  "main": "firestack",
  "repository": {
    "type": "git",
    "url": "https://github.com/fullstackreact/react-native-firestack.git"
  },
  "nativePackage": true,
  "scripts": {
    "compile": "babel --presets es2015,stage-0 -d . src/",
