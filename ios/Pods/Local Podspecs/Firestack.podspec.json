{
  "name": "Firestack",
  "version": "1.1.1",
  "summary": "Firestack makes working with Firebase v3 easy",
  "description": "Wanna integrate firebase into your app using React Native?",
  "homepage": "http://fullstackreact.com",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Ari Lerner": "Ari Lerner <ari@fullstack.io> (https://fullstackreact.com)"
  },
  "social_media_url": "http://twitter.com/fullstackio",
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/fullstackreact/react-native-firestack.git",
    "tag": "v1.1.1"
  },
  "public_header_files": [
    "ios/Firestack/*.h",
    "Pods/**/*.h"
  ],
  "source_files": "ios/Firestack/*.{h,m}",
  "preserve_paths": [
    "README.md",
    "package.json",
    "*.js"
  ],
  "default_subspecs": "Core",
  "ios": {
    "frameworks": [
      "SystemConfiguration",
      "CFNetwork",
      "Security",
      "UIKit",
      "CoreFoundation",
      "StoreKit",
      "MobileCoreServices"
    ],
    "libraries": [
      "c++",
      "sqlite3",
      "icucore",
      "z",
      "objc"
    ]
  },
  "requires_arc": true,
  "subspecs": [
    {
      "name": "Core",
      "dependencies": {
        "Firebase/Core": [

        ],
        "Firebase/Auth": [

        ],
        "Firebase/Storage": [

        ],
        "Firebase/Database": [

        ],
        "Firebase/RemoteConfig": [

        ]
      },
      "xcconfig": {
        "HEADER_SEARCH_PATHS": "$(inherited) ${SRCROOT}/../../React/** ${SRCROOT}/../node_modules/react-native/**",
        "FRAMEWORK_SEARCH_PATHS": "$(inherited) ${SRCROOT}/../../React/** ${SRCROOT}/../node_modules/react-native/**"
      }
    },
    {
      "name": "Test",
      "dependencies": {
        "Firestack/Core": [

        ]
      },
      "xcconfig": {
        "HEADER_SEARCH_PATHS": "$(inherited) ${SRCROOT}/../../React/** ${SRCROOT}/../node_modules/react-native/** $(SRCROOT)/../../react-native/React/**"
      }
    }
  ]
}
