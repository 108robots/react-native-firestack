{
  "name": "Firestack",
  "version": "0.0.2",
  "summary": "Firestack makes working with Firebase v3 easy",
  "description": "Wanna integrate firebase into your app using React Native?",
  "homepage": "http://fullstackreact.com",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Ari Lerner": "Ari Lerner <ari@fullstack.io> (https://fullstackreact.com)"
  },
  "social_media_url": "http://twitter.com/fullstackio",
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/fullstackreact/react-native-firestack.git",
    "tag": "v0.0.2"
  },
  "public_header_files": "ios/Firestack/*.h",
  "source_files": "ios/Firestack/*.{h,m}",
  "preserve_paths": [
    "README.md",
    "package.json",
    "*.js"
  ],
  "ios": {
    "frameworks": [
      "CFNetwork",
      "Security",
      "SystemConfiguration"
    ],
    "libraries": [
      "icucore",
      "c++",
      "sqlite3",
      "z"
    ]
  },
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(inherited) ${SRCROOT}/../../React/** ${SRCROOT}/../../node_modules/react-native/**",
    "FRAMEWORK_SEARCH_PATHS": "$(inherited) ${PODS_ROOT}/Firebase/** ${PODS_ROOT}/FirebaseStorage/**",
    "OTHER_LDFLAGS": "$(inherited) -ObjC"
  }
}
